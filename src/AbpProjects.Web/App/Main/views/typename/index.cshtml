<div class="row clearfix" ng-controller="app.views.typename.index as vm">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card main-content">
            <div class="header data_section">
                <h2>
                    @L("TypeName")
                </h2>
                 <div>
                  <ul>
                    <li>
                        <input type="text" ng-model="vm.searchBox" class="validate form-control" placeholder="Search by Type Name" style="height: 35px">
                    </li>
                    <li>
                        <button type="button" class="btn btn-primary" ng-click="vm.refreshGrid(vm.searchBox)" style="height: 35px"><i class="fa fa-search" aria-hidden="true"></i></button>
                    </li>
                      <li>
                         <button type="button" data-toggle="modal" class="btn btn-primary add" ng-click="vm.openTypenameCreationModal()">
                            <i class="material-icons">add</i>
                        </button>
                    </li>
                  
                </ul>
                        
                    </div>
            </div>
         

            <p ng-if="noData" class="alert alert-info"> No record found.</p>
            <div class="body table-responsive">
                <div ui-grid="vm.userGridOptions" ui-grid-pagination ui-grid-resize-columns full-height-grid dir="ltr" ng-if="!noData"></div>



            </div>
        </div>
    </div>
</div>
