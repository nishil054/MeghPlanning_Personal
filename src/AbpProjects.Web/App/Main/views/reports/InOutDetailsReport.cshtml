<div class="row clearfix" ng-controller="app.views.reports.inOutDetailsReport as vm">
    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
        <div class="card main-content">
            <div class="header">
                <h2>
                    @L("EmployeeInOutDetailsReport")
                </h2>
                <div class="search_container">
                    <ul>
                        <li>
                            <a style="color:#fff" class="btn btn-default btn_flex waves-effect" href="javascript:history.back()"><i class="fa fa-angle-left" aria-hidden="true"></i> @L("Back")</a>
                        </li>
                    </ul>
                </div>
            </div>

            <p ng-show="noData" class="alert alert-info"> No record found.</p>

            <div class="body table-responsive footer_table_right">
                <div ui-grid="vm.inoutGridOptions" ui-grid-pagination ui-grid-resize-columns full-height-grid dir="ltr" ng-show="!noData"></div>
            </div>
        </div>
    </div>
</div>