<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div class="card main-content">

        <div class="modal-header">

            <button type="button" class="close" ng-click="vm.close()" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>

            <h4 class="modal-title">
                <span>@L("CreateData")</span>
            </h4>
        </div>
        <div class="">
            <form id="frm_create" name="CreateForm" novalidate>
                <div class="modal-body">
                    <div class="row clearfix">
                        <div class="col-sm-6">
                            <div class="form-group form-float formRight">
                             <label class="form-label">@L("Team")<span style="color:red;">*</span></label>
                                <div class="form-line focused ">
                                    <select class="js-example-placeholder-single js-states form-control validate" ui-select2 ng-model="vm.managekc.teamId" ng-change="changeteam(vm.managekc.teamId)" required>
                                        <option value="">Select Team</option>
                                        <option ng-repeat="item in  vm.teamList" value="{{item.id}}">
                                            {{item.teamName}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group form-float formRight">
                              <label class="form-label">@L("Category")<span style="color:red;">*</span></label>
                                <div class="form-line focused ">
                                    <select class="js-example-placeholder-single js-states form-control validate"
                                            ui-select2 ng-model="vm.managekc.categoryId" required>
                                        <option value="">Select Category</option>
                                        <option ng-repeat="item in  vm.categoryList" value="{{item.id}}">
                                            {{item.category}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                        </div> 
                    </div>
                    @*<div class="row clearfix">
                            <div class="col-sm-12">
                                <div class="form-group form-float">
                                    <div class="form-line form-float">
                                        <input id="ltcheck" type="checkbox" name="ltcheck" ng-model="vm.managekc.isDocument" ng-click="vm.isdocselected(vm.managekc.isDocument)">
                                        <label for="ltcheck" class="form-label">Is Document Selected?</label>&nbsp;
                                    </div>
                                </div>
                            </div>

                        </div>*@
                    <div class="row clearfix">
                        <div class="col-sm-6">
                            <div class="form-group form-float">
                             <label for="title" class="form-label">@L("Title")<span style="color:red;">*</span></label>
                             <div class="form-line focused ">
                               
                                <input id="title" type="text" name="title" ng-model="vm.managekc.title" required class="validate form-control">
                            </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group">
                            <label class="form-label">Source</label>
                             <div class="form-line focused ">
                                
                                <select class="form-control" ng-change="vm.isdocselected(vm.managekc.isDocument)" data-placeholder="Select Revision" ng-model="vm.managekc.isDocument">
                                    @*<option value="">Select Source</option>*@
                                    <option ng-repeat="item in  vm.sourceList" value="{{item.sourceId}}">
                                        {{item.sourceName}}
                                    </option>
                                </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row clearfix">
                        
                        <div class="col-sm-12" ng-show="vm.showdoc == true">
                         <div class="form-line focused ">
                            <div class="uploadimbbox flexcolumn" style="max-width: 50%;">
                                <label for="attachment" class="form-label">@L("attachment")<span style="color:red;">*</span></label>
                                <div class="input-group">
                                    <input type="text" ng-model="vm.multidoc" class="form-control" placeholder="No file selected" readonly>
                                    <span class="input-group-btn">
                                        <div class="btn btn-primary  custom-file-uploader">
                                            <input id="file" type="file" name="file" multiple
                                                   onchange="angular.element(this).scope().getFileDetails(this)" />
                                            Browse
                                        </div>
                                    </span>
                                </div>
                                </div>
                            </div>
                             <span style="color: red; display:block;" class="form-group">(Upload file of type .xls, .xlsx, .pdf, .doc, .docx, .txt, .jpg, .jpeg, .png and .zip only.)</span>
                        </div>
                       
                        @*<div class="col-sm-6" ng-show="vm.showdoc == true">
                                <div class="uploadimbbox flexcolumn">
                                    <label for="attachment" class="form-label">@L("DocumentFile") <span style="color: red;">*</span></label>
                                    <div class="input-group">
                                        <input style="margin-block: 10px;" type="text" ng-model="vm.DocumentMasters.documentFile" class="validate form-control" required placeholder="No file selected" readonly>
                                        <span class="input-group-btn">
                                            <div class="btn btn-primary  custom-file-uploader">
                                                <input id="filetoupload" type="file" name="file" multiple
                                                    onchange="angular.element(this).scope().documentuploadvalidation(this)" />
                                                <input id="filetoupload" type="file" name="file" onchange="this.form.logo.value = this.files.length ? this.files[0].name : ''" />
                                                Browse


                                            </div>
                                        </span>
                                    </div>
                                </div>
                            </div>*@
                        <div class="col-sm-12" ng-show="vm.showdoc == false">
                            <div class="form-group form-float">
                            <label for="url" class="form-label">@L("Url")<span style="color:red;">*</span></label>
                             <div class="form-line focused ">
                                
                                <input id="url" type="text" name="url" ng-model="vm.managekc.url" class="form-control" ng-blur="vm.urlvalidation(vm.managekc.url)">
                            </div>
                            </div>
                        </div>
                    </div>
                    <div class="row clearfix">
                        <div class="col-sm-12">
                            <div class="form-group form-float">
                            <label for="comment" class="form-label">@L("Comment")</label>
                             <div class="form-line focused ">
                                
                                @*<input id="comment" type="text" name="title" ng-model="vm.managekc.comment" class="form-control">*@
                                <textarea id="comment" cols="10" type="text" name="comment" ng-model="vm.managekc.comment" class="form-control"></textarea>
                            </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default waves-effect" ng-click="vm.close()">
                        @L("Cancel")
                    </button>
                    <button type="submit" id="btnsave" class="btn btn-primary waves-effect" ng-click="vm.save()" ng-disabled="CreateForm.$invalid || vm.loading">
                        @L("Save")
                    </button>
                </div>

            </form>
        </div>
    </div>
</div>