

<div class="modal-header">
    <button type="button" class="close" ng-click="vm.cancel()" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>

    <h4 class="modal-title">@L("CategoryDetails")</h4>
</div>
<div class="modal-body detailmodal">
    <div class="row">
        <div class="col-md-12">
            <label for="categoryName" class="form-label">@L("Category")</label>
            <p> {{vm.items.category}}</p>
        </div>
        <div class="col-md-12">
            <label for="teamName" class="form-label">Team</label>
            <p> {{vm.items.teamname}}</p>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-default waves-effect" ng-click="vm.cancel()">
        @L("Cancel")
    </button>
</div>




