@using System.Threading
@using System.Web.Optimization
@using Abp.Timing
@using Abp.Web.Mvc.Extensions
@{
    Layout = null;
    SetAntiForgeryCookie();
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>Megh Planning</title>
    <link rel="shortcut icon" href="~/favicon.ico">
    @Styles.Render("~/Bundles/App/vendor/css")
    @Styles.Render("~/Bundles/App/Main/css")
    @*permission related theme*@
    @Styles.Render("~/Bundles/App/css/themes")
    <!-- theme css -->
    <link rel="stylesheet" href="~/css/customtheme.css" />
    <script type="text/javascript">
        //This is used to get the application's root path from javascript. It's useful if you're running application in a virtual directory under IIS.
        var abp = abp || {}; abp.appPath = '@ApplicationPath';
    </script>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="@Url.Content("~/Scripts/others/html5shiv.js")"></script>
        <script src="@Url.Content("~/Scripts/others/respond.min.js")"></script>
    <![endif]-->
    @Scripts.Render("~/Bundles/App/vendor/js")
    @*permission related js*@
    @Scripts.Render("~/Bundles/App/metronic/js")
    <script>
        //Localizing momentjs
        moment.locale('@Thread.CurrentThread.CurrentUICulture.Name');
    </script>
    <!-- Dynamic scripts of ABP system (They are created on runtime and can not be bundled) -->
    <script src="~/api/AbpServiceProxies/GetAll?v=@(Clock.Now.Ticks)"></script>
    <script src="~/api/AbpServiceProxies/GetAll?type=angular&v=@(Clock.Now.Ticks)"></script>
    <script src="~/AbpScripts/GetScripts?v=@(Clock.Now.Ticks)" type="text/javascript"></script>
    <!-- SignalR scripts -->
    <script src="~/signalr/hubs"></script>
    @Html.IncludeScript("~/Abp/Framework/scripts/libs/abp.signalr.js")
    @*permission related js*@
    <script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/lodash.js/0.10.0/lodash.min.js"></script>
    <script src="~/js/jstree.min.js"></script>
    <script src="~/js/jstree.js"></script>

    @Scripts.Render("~/Bundles/App/Main/js")

    <link href="~/css/custom.css" rel="stylesheet" />
    @*<link href="~/css/select2.css" rel="stylesheet" />*@
    <link href="https://fonts.googleapis.com/css2?family=Material+Icons" rel="stylesheet">
    @******************** Css related to datepicker /Bundles/home-index" *@
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet">
    <link href="//rawgit.com/indrimuska/angular-moment-picker/master/dist/angular-moment-picker.css" rel="stylesheet">

    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.2/css/bootstrap3/bootstrap-switch.min.css" />

    @******************** Css related to datepicker ************************ *@
    @******************** Js related to datepicker ************************ *@
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment-with-locales.js"></script>
    <script src="//rawgit.com/indrimuska/angular-moment-picker/master/dist/angular-moment-picker.js"></script>
    @******************** Js related to datepicker ************************ *@
    <link data-semver="3.4.2" rel="stylesheet" href="https://rawgithub.com/ivaynberg/select2/3.4.2/select2.css" />
    <script data-semver="3.4.2" src="https://rawgithub.com/ivaynberg/select2/3.4.2/select2.js"></script>
    <script src="~/lib/morris.js/morris.js"></script>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-switch/3.3.2/js/bootstrap-switch.min.js" />
</head>
<body class="theme-red ls-closed" ng-app="app">
    <div class="overlay"></div>
    <div ng-controller="app.views.layout as vm">
        <div ng-include="'/App/Main/views/layout/topbar.cshtml'" onload="vm.activateTopBar()"></div>
        <section>
            <aside id="leftsidebar" class="sidebar">
                <div ng-include="'/App/Main/views/layout/sidebar-user-area.cshtml'" onload="vm.activateLeftSideBar()"></div>
                <div ng-include="'/App/Main/views/layout/sidebar-nav.cshtml'"></div>
                <div ng-include="'/App/Main/views/layout/sidebar-footer.cshtml'"></div>
            </aside>

            <div ng-include="'/App/Main/views/layout/right-sidebar.cshtml'" onload="vm.activateRightSideBar()"></div>
        </section>
        <section class="content" style="margin-left:70px;">
            <div class="container-fluid">
                <div ui-view></div>
            </div>
        </section>
    </div>
</body>
</html>