<div>
    <div class="modal-header">
        <h4 class="modal-title">
            <span ng-if="!vm.user.id">AuditLogDetail</span>
        </h4>
    </div>
    <div class="modal-body">
        <form class="form-horizontal audit-log-detail-view" role="form">
            <div class="form-body">
                <h3 class="form-section">UserInformations</h3>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label col-sm-3">UserName:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.userName}}
                                </p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3">IpAddress:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.clientIpAddress}}
                                </p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3">Client:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.clientName}}
                                </p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3">Browser:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.browserInfo}}
                                </p>
                            </div>
                        </div>
                        <div class="form-group" ng-if="vm.auditLog.impersonatorUserId">
                            <label class="control-label col-sm-3"></label>
                            <div class="col-sm-9">
                                <p class="form-control-static text-warning">
                                    AuditLogImpersonatedOperationInfo
                                </p>
                            </div>
                        </div>
                    </div>
                </div>

                <h3 class="form-section mt40">ActionInformations</h3>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label col-sm-3">Service:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.serviceName}}
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label col-sm-3">Action:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.auditLog.methodName}}
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label col-sm-3">Time:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.getExecutionTime()}}
                                </p>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="control-label col-sm-3">Duration:</label>
                            <div class="col-sm-9">
                                <p class="form-control-static">
                                    {{vm.getDurationAsMs()}}
                                </p>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-sm-3">Parameters:</label>
                            <div class="col-sm-9">
                                <pre lang="js">{{vm.getFormattedParameters()}}</pre>
                            </div>
                        </div>
                    </div>
                </div>

                <h3 class="form-section mt40">CustomData</h3>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="col-sm-12">
                                <p class="form-control-static" ng-if="!vm.auditLog.customData">
                                    None
                                </p>
                                <pre ng-if="vm.auditLog.customData">{{vm.auditLog.customData}}</pre>
                            </div>
                        </div>
                    </div>
                </div>

                <h3 class="form-section">ErrorState</h3>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <div class="col-sm-12">
                                <p class="form-control-static" ng-if="!vm.auditLog.exception">
                                    <i class="fa fa-check-circle font-green"></i> Success
                                </p>
                                <pre ng-if="vm.auditLog.exception">{{vm.auditLog.exception}}</pre>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </form>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-danger" ng-click="vm.close()"> <i class="fa fa-times"></i> <span>Close</span></button>
    </div>
</div>