<form id="frm_history" name="historyForm" novalidate>
    <div class="modal-header">
        <button type="button" class="close" ng-click="vm.cancel()" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title">
            Detail
        </h4>


    </div>
    <div class="modal-body detailmodal">
        <div class="row">
            <div class="col-md-12">
                <label for="categoryName" class="form-label">@L("Category")</label>
                <div> {{vm.history.category}}</div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <label for="categoryName" class="form-label">@L("SubCategory")</label>
                <div> {{vm.history.subCategory}}</div>
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-default waves-effect" ng-click="vm.cancel()">
            @L("Cancel")
        </button>
    </div>
</form>